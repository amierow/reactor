<div class="page-header">
  <h1>
    Leads
    <a href="/leads/new" class="btn btn-lg btn-success">New Lead</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Email</th>
        <th>Password</th>
        <th>Username</th>
        <th>Actions</th>
      </tr>

      <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.email %></td>
        <td><%= lead.password %></td>
        <td><%= lead.username %></td>
        <td>
          <a href="/leads/<%= lead.id %>" class="btn btn-primary">Show</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
