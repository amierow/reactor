<div class="page-header">
  <h3>Insight #<%= @insight.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Presentation</dt>
      <dd><%= @insight.presentation_id %></dd>

      <dt>Contributor</dt>
      <dd><%= @insight.contributor_id %></dd>

      <dt>Insght type</dt>
      <dd><%= @insight.insght_type_id %></dd>

      <dt>Title</dt>
      <dd><%= @insight.title %></dd>

      <dt>Body</dt>
      <dd><%= @insight.body %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/insights" class="btn btn-primary">
        Back
      </a>
      <a href="/insights/<%= @insight.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_insight/<%= @insight.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
