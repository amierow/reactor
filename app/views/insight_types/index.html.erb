<div class="page-header">
  <h1>
    Insight types
    <a href="/insight_types/new" class="btn btn-lg btn-success">New Insight type</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Insight type</th>
        <th>Actions</th>
      </tr>

      <% @insight_types.each do |insight_type| %>
      <tr>
        <td><%= insight_type.insight_type %></td>
        <td>
          <a href="/insight_types/<%= insight_type.id %>" class="btn btn-primary">Show</a>
          <a href="/insight_types/<%= insight_type.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_insight_type/<%= insight_type.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @insight_types, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
